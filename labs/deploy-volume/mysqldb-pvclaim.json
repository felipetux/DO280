{
    "apiVersion": "v1",
    "kind": "PersistentVolumeClaim",
    "metadata": {
        "name": "mysqldb-pvclaim",
        "labels": {
            "name": "mysqldb"
        }
    },
    "spec": {
        "accessModes": [ "ReadWriteMany" ],
        "resources": {
            "requests": {
                "storage": "3Gi"
            }
        }
    }
}
